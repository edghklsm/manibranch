package pack



object obj {
  
  def main(args:Array[String]):Unit={
    
    
    println("===started===")
    
    
    val a  =  2
    
    val b  =  3
    
    val c  =  "zeyobron"
    
    val lisin = List( 1 , 2 , 3 , 4)
    
    
    println(a)
    
    println(b)
    
    println(c)
    
    println(lisin)
    
    lisin.foreach(println)
   
  }
  
}
